<!DOCTYPE html>
<!-- Website template by freewebsitetemplates.com -->
<html>
<head>
	<meta charset="UTF-8">
	<title>Rooms - Travlr Getaways Website Template</title>
	<link rel="stylesheet" href="css/style.css" type="text/css">
</head>
<body>
	<div id="background">
		<div id="page">
			{{> header}}
			<div id="contents">
				<div class="box">
					<div>
						<div class="body">
							<h1>Rooms</h1>
			{{#each roomsList}}
							<ul id="rooms">
								<li>
									<a href="{{this.href}}"><img src="images/{{this.img}}" alt="Img"></a>
									<h2><a href="{{this.href}}">{{this.title}}</a></h2>
									<p>
										{{this.content}}
									</p>
									<span class="rate">Rate: {{this.rate}} / Day</span>
								</li>
							</ul>
			{{/each}}
						</div>
					</div>
				</div>
			</div>
		</div>
		{{> footer}}
	</div>
</body>
</html>